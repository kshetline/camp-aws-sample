{
  "version": "0.2.0",
  "configurations": [{
      "name": "Debug get-timezone",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 5858,
      "localRoot": "${workspaceRoot}/dist/get-timezone",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false,
      "preLaunchTask": "local-tz-debug",
      "outFiles": [
        "${workspaceRoot}/dist/get-timezone/**/*.js"
      ],
      "sourceMaps": true
    },
    {
      "name": "Debug reverse",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 5858,
      "localRoot": "${workspaceRoot}/dist/reverse",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false,
      "preLaunchTask": "local-r-debug",
      "outFiles": [
        "${workspaceRoot}/dist/reverse/**/*.js"
      ],
      "sourceMaps": true
    }
  ]
}
